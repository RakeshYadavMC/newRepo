<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Template_Product_Name__c</fullName>
    <description>This field shows Product name if the product is present on Case otherwise string "Product"</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISBLANK(TEXT(Product_Type__c))), TEXT(Product_Type__c), "Product")</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Template Product Name</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
