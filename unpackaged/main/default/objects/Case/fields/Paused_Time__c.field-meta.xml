<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Paused_Time__c</fullName>
    <description>This field will calculate the time for which the milestone is paused in hours.</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISNULL(StopStartDate)), (NOW() - StopStartDate)*24, null)</formula>
    <inlineHelpText>Milestone paused time in hours.</inlineHelpText>
    <label>Paused Time</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
