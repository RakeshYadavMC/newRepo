<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Allocation_Used__c</fullName>
    <description>PS-MC-SB-13 to manage percentage of used allocations for this time period</description>
    <externalId>false</externalId>
    <formula>IF( Allocations_Per_Interval__c &gt; 0, Used_Allocations__c / Allocations_Per_Interval__c, NULL)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>% Allocation Used</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
