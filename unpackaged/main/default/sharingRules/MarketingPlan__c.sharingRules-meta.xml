    <sharingCriteriaRules>
        <fullName>AUS_Marketing_Plan_Sharing</fullName>
        <accessLevel>Edit</accessLevel>
        <label>AUS Marketing Plan Sharing</label>
        <sharedTo>
            <group>All_AUS_Marketing_Users</group>
        </sharedTo>
        <criteriaItems>
            <field>Country__c</field>
            <operation>equals</operation>
            <value>AUS</value>
        </criteriaItems>
    </sharingCriteriaRules>
